    
    <h2 class="mb-2">Liste des participants inscrits :</h2>
    <table class="table">
        <thead>
            <tr>
                <th>Pseudo</th>
                <th>Name</th>
            </tr>
        </thead> 
        <tbody>
            {% for participant in participants %}
            <tr>
                <td>{{ participant.id }}</td>
                <td>{{ participant.firstname }} {{ participant.name }}</td>
            </tr>
            {% endfor %}
        </tbody>
    </table>